<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NetCord" Version="1.0.0-alpha.428" />
	<PackageReference Include="NetCord.Hosting" Version="1.0.0-alpha.428" />
    <PackageReference Include="NetCord.Hosting.AspNetCore" Version="1.0.0-alpha.428" />
    <PackageReference Include="NetCord.Hosting.Services" Version="1.0.0-alpha.428" />
    <PackageReference Include="NetCord.Services" Version="1.0.0-alpha.428" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Commands.cs">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Compile>
  </ItemGroup>

	<ItemGroup>
		<Compile Update="BlackJack.cs">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Compile>
	</ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
